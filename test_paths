    // Pipeline Starts Here
    pipeline {
        agent {
            label pipelineParams.nodeName
        }//end of agent
        stages {
            stage("execute tests") {
                steps {
                    script {
                        println("Start of execute test")
                        try {

                            bat """
                           	export PYTHONPATH=\$PATH:\$(pwd)
                            echo "${env.WORKSPACE}
                        	echo "${PYTHONPATH}"

                        	"""
                        } catch (err) {
                            println "Some Error while running the PNS_RATE_FINDER job"
                            errorCondition = "true"
                        }
                    }//end of script
                }//end of steps
            }//end of stage
        }//end of stages
}//end of pipeline
