    // Pipeline Starts Here
    pipeline {
    agent any
        stages {
            stage("execute tests") {
                steps {
                    script {
                        println("Start of execute test")
                        try {

                            bat """
                            echo "workspace is : ${env.WORKSPACE}"
                           	set PYTHONPATH=\$PATH:\$(pwd)                            
                        	echo " python path is : ${PYTHONPATH}"

                        	"""
                        } catch (err) {
                            println ("Some Error while running the task\n err:"+err)
                            
                        }
                    }//end of script
                }//end of steps
            }//end of stage
        }//end of stages
}//end of pipeline
