    // Pipeline Starts Here
    pipeline {
    agent any
        stages {
            stage("execute tests") {
                steps {
                    script {
                        println("Start of execute test")
                        try {

                            bat """
                            echo "workspace is : ${env.WORKSPACE}"
                            echo " Jenkins path is ${env.PATH}" 
                            echo " System Path is :"
                            echo %Path%
                            echo %PYTHONPATH%
                            export PYTHONPATH=$WORKSPACE:$PYTHONPATH
                            echo "${PYTHONPATH}"
                        	echo " python path is :"
                            echo %PYTHONPATH%
                            python --version
                            

                        	"""
                        } catch (err) {
                            println ("Some Error while running the task\n err:"+err)
                            
                        }
                    }//end of script
                }//end of steps
            }//end of stage
        }//end of stages
}//end of pipeline
